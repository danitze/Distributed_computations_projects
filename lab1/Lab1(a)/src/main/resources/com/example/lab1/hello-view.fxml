<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.lab1.MainController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Slider fx:id="slider" showTickLabels="true" />
   <HBox prefWidth="200.0">
      <children>
         <Spinner fx:id="firstThreadPrioritySpinner" />
         <Separator opacity="0.0" prefHeight="6.0" prefWidth="55.0" />
         <Spinner fx:id="secondThreadPrioritySpinner" />
      </children>
   </HBox>
   <Button fx:id="startButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Start" />
</VBox>
